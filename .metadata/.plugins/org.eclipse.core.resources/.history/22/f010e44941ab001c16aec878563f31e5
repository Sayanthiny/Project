package com.employee.operation.dto;

public class ApplyLeaveDto {
	
	private String reason;
	private int leave_count;
	private int leave_type_id;
	private int employee_id;
	public String getReason() {
		return reason;
	}
	public void setReason(String reason) {
		this.reason = reason;
	}
	public int getLeave_count() {
		return leave_count;
	}
	public void setLeave_count(int leave_count) {
		this.leave_count = leave_count;
	}
	public int getLeave_type_id() {
		return leave_type_id;
	}
	public void setLeave_type_id(int leave_type_id) {
		this.leave_type_id = leave_type_id;
	}
	public int getEmployee_id() {
		return employee_id;
	}
	public void setEmployee_id(int employee_id) {
		this.employee_id = employee_id;
	}
	
	
	

}
